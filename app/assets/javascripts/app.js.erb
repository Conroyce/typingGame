var app = angular.module("TypingGame",["ngRoute"])
  .config(["$routeProvider", function($routeProvider) {
    $routeProvider
      .when("/", {
        templateUrl: "<%= asset_path('templates/views/game.html') %>",
        controller: "GameCtrl"
      })
      .when("/landing", {
        templateUrl: "<%= asset_path('templates/views/landing.html') %>",
        controller: "LandCtrl"
      })
      .otherwise({
        redirectTo: "/"
      });
  }]);